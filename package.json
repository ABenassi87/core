{
  "name": "rucken",
  "version": "2.1.1",
  "engines": {
    "node": ">=8",
    "npm": ">=6.1.0"
  },
  "private": true,
  "license": "MIT",
  "author": "EndyKaufman <admin@site15.ru>",
  "description": "Core with Admin UI for web and native application maked on Angular6+",
  "bugs": {
    "url": "https://github.com/rucken/core/issues"
  },
  "homepage": "https://github.com/rucken/core",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rucken/core.git"
  },
  "keywords": [
    "ngx",
    "angular6",
    "admin ui",
    "web application",
    "seed",
    "i18n",
    "bootstrap",
    "ssr"
  ],
  "maintainers": [
    {
      "name": "EndyKaufman",
      "email": "admin@site15.ru"
    }
  ],
  "scripts": {
    "tscfg:dev": "cp-cli ./tsconfig.dev.json ./tsconfig.json",
    "tscfg:prod": "cp-cli ./tsconfig.prod.json ./tsconfig.json",
    "lib:prepare-core": "rucken prepare --lib ./libs/rucken/core",
    "lib:prepare-web": "rucken prepare --lib ./libs/rucken/web",
    "lib:build-core": "ng build @rucken/core --prod",
    "lib:build-web": "ng build @rucken/web --prod",
    "lib:prepare": "run-s lib:prepare-core lib:prepare-web",
    "lib:build": "run-s lib:prepare lib:build-core lib:build-web",
    "app:prepare": "rucken prepare --app ./apps/demo",
    "app:prebuild": "ng build demo --prod",
    "app:prebuild-mockapi": "ng build demo --prod -c=mockapi",
    "app:prebuild-github": "ng build demo --base-href /core/ --prod -c=mockapi",
    "app:build": "run-s lib:build app:prepare app:prebuild",
    "app:build-mockapi": "run-s lib:build app:prepare app:prebuild-mockapi",
    "app:build-github": "run-s lib:build app:prepare app:prebuild-github",
    "app:predeploy-github": "cp-cli ./dist/demo/index.html ./dist/demo/404.html",
    "app:deploy-github": "gh-pages -d dist/demo",
    "lite-server": "lite-server -c bs-config.json",
    "ng": "ng",
    "serve": "ng serve",
    "serve-mockapi": "ng serve -c=mockapi",
    "start": "run-s tscfg:dev lib:prepare serve",
    "start-mockapi": "run-s tscfg:dev lib:prepare serve-mockapi",
    "build": "run-s tscfg:dev app:build",
    "build-mockapi": "run-s tscfg:prod app:build-mockapi",
    "build-github": "run-s tscfg:prod app:build-github app:predeploy-github",
    "test": "npm run app:build",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "postinstall": "node scripts/patch.js",
  "dependencies": {
    "@angular/animations": "^6.0.3",
    "@angular/common": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/forms": "^6.0.3",
    "@angular/http": "^6.0.3",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/platform-server": "^6.0.0",
    "@angular/router": "^6.0.3",
    "@ng-toolkit/universal": "^1.1.16",
    "@nguniversal/express-engine": "^6.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^6.0.0",
    "@ngx-translate/core": "^10.0.2",
    "@ngx-utils/cookies": "^3.0.1",
    "@types/jsonwebtoken": "^7.2.7",
    "@types/object-path": "^0.9.29",
    "core-js": "^2.5.4",
    "cors": "~2.8.4",
    "jsonwebtoken": "^8.2.2",
    "ngx-bootstrap": "^3.0.0",
    "ngx-cookie-service": "^1.0.10",
    "ngx-dynamic-form-builder": "^0.4.2",
    "ngx-permissions": "^5.0.0",
    "ngx-repository": "^0.4.1",
    "object-path": "^0.11.4",
    "rxjs": "^6.0.0",
    "ts-loader": "4.2.0",
    "webpack-cli": "^2.1.4",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.6",
    "@angular-devkit/build-ng-packagr": "~0.6.6",
    "@angular/cli": "~6.0.7",
    "@angular/compiler-cli": "^6.0.3",
    "@angular/language-service": "^6.0.3",
    "@rucken/cli": "^2.0.6",
    "@types/body-parser": "^1.17.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "bootstrap-sass": "^3.3.7",
    "bootswatch-sass": "^3.2.0",
    "codelyzer": "~4.2.1",
    "conventional-changelog-cli": "^2.0.0",
    "conventional-commits-detector": "^0.1.1",
    "conventional-recommended-bump": "^3.0.0",
    "cookie-parser": "^1.4.3",
    "cp-cli": "^1.1.2",
    "domino": "^2.0.2",
    "font-awesome": "^4.7.0",
    "gh-pages": "^1.2.0",
    "html-loader": "^0.5.5",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^2.0.2",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lite-server": "^2.3.0",
    "markdown-loader": "^3.0.0",
    "ng-packagr": "^3.0.1",
    "npm-run-all": "^4.1.3",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tsickle": ">=0.25.5",
    "tslib": "^1.7.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  },
  "greenkeeper": {
    "ignore": [
      "typescript",
      "ts-loader"
    ]
  }
}