{
  "name": "@rucken/core",
  "version": "1.17.0",
  "license": "MIT",
  "author": "EndyKaufman <admin@site15.ru>",
  "engines": {
    "node": ">=6",
    "npm": ">=5.3.0"
  },
  "private": true,
  "description": "Core with Admin UI for web and native application maked on Angular4",
  "bugs": {
    "url": "https://github.com/rucken/core/issues"
  },
  "homepage": "https://github.com/rucken/core",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rucken/core.git"
  },
  "main": "bundles/rucken.umd.js",
  "module": "index.js",
  "typings": "index.d.ts",
  "keywords": [
    "ngx",
    "angular4",
    "admin ui",
    "web application",
    "seed",
    "i18n",
    "bootstrap"
  ],
  "maintainers": [
    {
      "name": "EndyKaufman",
      "email": "admin@site15.ru"
    }
  ],
  "scripts": {
    "core:link": "ngm link -p core --here",
    "core:build": "ngm build -p ./core --clean",
    "core:link-npm": "npm link core",
    "core:link-npm-clear": "./node_modules/.bin/del-cli ./core/node_modules",
    "core:tools-change_version": "gulp core:change-version",
    "core:tools-extract_translate": "ngx-translate-extract --input ./core --output ./core/i18n/template.pot --format=pot --marker translate --clean",
    "core:tools-po2ts": "./node_modules/.bin/srcgen -x -t convert.po.to.ts -f ./srcgen/core-convert.po.to.ts.json",
    "core:tools-make_ts_list": "./node_modules/.bin/srcgen -x -t make.list.ts.files -f ./srcgen/core-make.list.ts.files.json",
    "core:tools-prepare": "npm-run-all core:tools-extract_translate core:tools-po2ts core:tools-make_ts_list",
    "web:link": "ngm link -p web --here",
    "web:build": "ngm build -p ./web --clean",
    "web:link-npm": "npm link web",
    "web:link-npm-clear": "./node_modules/.bin/del-cli ./web/node_modules",
    "web:tools-change_version": "gulp web:change-version",
    "web:tools-po2ts": "./node_modules/.bin/srcgen -x -t convert.po.to.ts -f ./srcgen/web-convert.po.to.ts.json",
    "web:tools-make_ts_list": "./node_modules/.bin/srcgen -x -t make.list.ts.files -f ./srcgen/web-make.list.ts.files.json",
    "web:tools-extract_translate": "ngx-translate-extract --input ./web --output ./web/i18n/template.pot --format=pot --marker translate --clean",
    "web:tools-prepare": "npm-run-all web:tools-extract_translate web:tools-po2ts web:tools-make_ts_list",
    "app:github_pages": "npm-run-all app:github_full_build app:github_deploy",
    "app:github_build": "ng build --base-href https://rucken.github.io/core/ --prod --aot --build-optimizer",
    "app:github_full_build": "npm-run-all libs:build libs:link app:github_build app:tools-add_version",
    "app:github_deploy": "gh-pages -d demo/dist",
    "app:lite_server": "lite-server -c demo/bs-config.json",
    "app:serve_dev": "ng serve --host 0.0.0.0 --aot=false",
    "app:serve_prod": "ng serve --host 0.0.0.0 --aot",
    "app:serve_mockapi": "ng serve --host 0.0.0.0 --aot --env=mockapi",
    "app:prod_build": "ng build --base-href / --prod --aot --build-optimizer",
    "app:prod_full_build": "npm-run-all libs:build libs:link app:prod_build app:tools-add_version",
    "app:tools-add_version": "gulp app:add-version",
    "libs:tools-change_version": "npm-run-all core:tools-change_version web:tools-change_version",
    "libs:tools-prepare": "npm-run-all core:tools-prepare web:tools-prepare",
    "libs:build": "npm-run-all libs:tools-change_version core:build core:link web:build",
    "libs:link": "npm-run-all core:link web:link",
    "libs:link-npm": "npm-run-all core:link-npm web:link-npm core:link-npm-clear web:link-npm-clear",
    "start:dev": "npm-run-all libs:tools-prepare libs:link-npm app:serve_dev",
    "start:prod": "npm-run-all libs:tools-prepare libs:link-npm app:serve_prod",
    "start:mockapi": "npm-run-all libs:tools-prepare libs:link-npm app:serve_mockapi",
    "ng": "ng",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test": "npm-run-all lint app:github_full_build",
    "build": "npm-run-all app:github_full_build",
    "start": "npm-run-all start:mockapi"
  },
  "dependencies": {
    "@angular/animations": "^4.4.5",
    "@angular/common": "^4.4.5",
    "@angular/compiler": "^4.4.5",
    "@angular/core": "^4.4.5",
    "@angular/forms": "^4.4.5",
    "@angular/http": "^4.4.5",
    "@angular/platform-browser": "^4.4.5",
    "@angular/platform-browser-dynamic": "^4.4.5",
    "@angular/router": "^4.4.5",
    "@ngui/auto-complete": "^0.16.0",
    "@ngx-translate/core": "^8.0.0",
    "@types/jasmine": "2.6.0",
    "@types/lodash": "^4.14.77",
    "@types/marked": "0.3.0",
    "@types/node": "^8.0.34",
    "@types/requirejs": "^2.1.31",
    "@types/webpack": "^3.0.13",
    "angular2-jwt": "^0.2.3",
    "angular2-ladda": "^1.2.2",
    "angular2-text-mask": "^8.0.4",
    "awesome-typescript-loader": "^3.2.3",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.1",
    "intl": "^1.2.5",
    "lodash": "^4.17.4",
    "moment": "^2.19.1",
    "ngx-bootstrap": "^1.9.3",
    "rxjs": "^5.4.3",
    "text-mask-addons": "^3.7.0",
    "tslib": "^1.8.0",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "^1.4.7",
    "@angular/compiler-cli": "^4.4.5",
    "@biesbjerg/ngx-translate-extract": "^2.3.2",
    "angular2-template-loader": "^0.6.2",
    "codelyzer": "~3.2.1",
    "conventional-changelog-cli": "^1.3.4",
    "conventional-commits-detector": "^0.1.1",
    "conventional-recommended-bump": "^1.0.2",
    "del-cli": "^1.1.0",
    "es6-promise-loader": "^1.0.2",
    "gh-pages": "^1.0.0",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-replace": "^0.6.1",
    "gulp-tsc": "^1.3.2",
    "html-loader": "0.5.1",
    "jasmine-core": "^2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lite-server": "^2.3.0",
    "locate-path": "^2.0.0",
    "markdown-loader": "^2.0.1",
    "ngm-cli": "^0.8.2",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "po2json": "^0.4.5",
    "protractor": "~5.1.2",
    "recursive-readdir": "^2.2.1",
    "shebang-command": "^1.2.0",
    "srcgen": "^0.3.0",
    "ts-node": "~3.3.0",
    "tslint": "~5.7.0",
    "typescript": "~2.5.3",
    "typings": "^2.1.1"
  }
}
