{
  "_description": "if error on npm run build, try use it: cd src && ../node_modules/.bin/ngc -p tsconfig.aot.json",
  "compilerOptions": {
    "skipLibCheck": true,
    "outDir": "../aot",
    "target": "es5",
    "module": "es2015",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [
      "dom",
      "es2015"
    ],
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true,
    "typeRoots": [
      "./node_modules/@types"
    ],
    "types": [
      "node"
    ]
  },
  "files": [
    "../typings.d.ts",
    "./index.ts"
  ],
  "angularCompilerOptions": {
    "genDir": "../aot",
    "skipMetadataEmit": true
  }
}
